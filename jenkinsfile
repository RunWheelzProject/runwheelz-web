pipeline{
    agent any
    stages{
        stage('git repo & clean'){
            steps{
                bat "rmdir /s/q runwheelz-web"
                bat "git clone https://github.com/RunWheelzProject/runwheelz-web.git"
                bat "mvn clean -f runwheelz-web"
            }
        }
        stage('install'){
            steps{
                bat "mvn install -f runwheelz-web"
            }
        }
        stage('test'){
            steps{
                bat "mvn test -f runwheelz-web"
            }
        }
        stage('package'){
            steps{
                bat  "mvn package -f runwheelz-web"
            }
        }
        
    }
}